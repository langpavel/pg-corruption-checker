{
  "name": "@langpavel/pg-corruption-checker",
  "version": "0.0.1",
  "description": "PostgreSQL/TimescaleDB Corruption Checker",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts",
    "./cli": "./pg-corruption-checker.ts"
  },
  "tasks": {
    "dev": "deno run --watch --allow-net --allow-env --allow-read pg-corruption-checker.ts",
    "run": "deno run --allow-net --allow-env --allow-read pg-corruption-checker.ts",
    "test": "deno test --allow-net --allow-env --allow-read"
  },
  "imports": {
    "postgresjs": "https://deno.land/x/postgresjs@v3.4.5/mod.js",
    "@std/assert": "jsr:@std/assert",
    "@std/fmt": "jsr:@std/fmt",
    "@std/cli": "jsr:@std/cli",
    "@std/fs": "jsr:@std/fs",
    "@std/path": "jsr:@std/path",
    "@std/toml": "jsr:@std/toml"
  }
}
